# EC2 instance variable

ec2:
  region: us-east-2
  credentials: essentialskeypair
  image: ami-922914f7
  sg: launch-wizard-3
  instance_type: t2.micro
  subnet: subnet-5a450032
  public_ip: yes
  tags:
    Name: Ec2Ansible
  volumes:
    - device_name: /dev/xvda
      volume_type: standard
      volume_size: 8
  



